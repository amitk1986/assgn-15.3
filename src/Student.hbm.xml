<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.diablo.entity.Student" table="student">
   <id name="stid" type="int" column="STUD_ID">
   	<generator class="increment"/>
   </id>
   <property name="name" column="STUD_NAME" type="string"/>
   <property name="marks" column="STUD_MARKS" type="int"/>
   <property name="joindate" column="STUD_JOIN_DATE" type="date"/>
   </class>
</hibernate-mapping>